{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "app/**/*"
    //"**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "scripts/"
  ]
}
